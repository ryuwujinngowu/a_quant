# 阿里云RDS配置
DB_HOST=
DB_PORT=
DB_USER=
DB_PASSWORD=
DB_NAME=a_quant
# 连接池配置（性能优化）
DB_MIN_CONNS=5
DB_MAX_CONNS=20
DB_CONNECT_TIMEOUT=30
# ========== Tushare配置（新增） ==========
TS_TOKEN=
# ========== 语言模型配置（新增） ==========
DOUBAO_API_KEY=
DOUBAO_API_URL=https://ark.cn-beijing.volces.com/api/v3/chat/completions
DOUBAO_MODEL=doubao-1-5-pro-32k-250115  # 完全对齐调试页面的model名
DOUBAO_SYSTEM_PROMPT=你是人工智能助手.  # 新增system prompt配置
# 重试配置
DOUBAO_RETRY_TIMES=3
DOUBAO_RETRY_DELAY=1
# Token统计开关
DOUBAO_TOKEN_STAT=True


# ========== 日志配置 ==========
LOG_LEVEL=INFO
# 日志等级：DEBUG/INFO/WARNING/ERROR/CRITICAL
LOG_FILE_PATH=logs/quant_db.log
# 日志文件存储路径（自动创建目录）
LOG_MAX_BYTES=10485760
# 单个日志文件最大大小（10MB）
LOG_BACKUP_COUNT=5
# 日志备份文件数量（最多保留5个）
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(filename)s:%(lineno)d - %(message)s
# 日志格式
LOG_DATE_FORMAT=%Y-%m-%d %H:%M:%S
# 日志时间格式
